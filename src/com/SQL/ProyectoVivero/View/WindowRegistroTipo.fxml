<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #2ca96c;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.SQL.ProyectoVivero.View.RegistroTipoController">
   <children>
      <TableView fx:id="tableTipo" layoutX="96.0" layoutY="85.0" prefHeight="230.0" prefWidth="409.0" style="-fx-background-color: #61c4e4;">
        <columns>
          <TableColumn fx:id="columnId" prefWidth="180.0" text="ID" />
          <TableColumn fx:id="columnTipo" prefWidth="228.0" text="TIPO" />
        </columns>
      </TableView>
      <AnchorPane prefHeight="25.0" prefWidth="600.0" style="-fx-background-color: #61c4e4;">
         <children>
            <Label layoutX="193.0" layoutY="-1.0" text="TIPOS DE PRODUCTOS">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <HBox layoutX="84.0" layoutY="350.0" prefHeight="36.0" prefWidth="433.0" spacing="40.0">
         <children>
            <Button fx:id="btnVer" mnemonicParsing="false" onAction="#clickVer" prefHeight="36.0" prefWidth="82.0" style="-fx-background-color: #61c4e4;" text="Ver">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="btnAgregar" mnemonicParsing="false" onAction="#clickCrear" prefHeight="36.0" prefWidth="79.0" style="-fx-background-color: #61c4e4;" text="Agregar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="btnBorrar" mnemonicParsing="false" onAction="#clickBorrar" prefHeight="36.0" prefWidth="75.0" style="-fx-background-color: #61c4e4;" text="Borrar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="btnModificar" mnemonicParsing="false" onAction="#clickModificar" prefHeight="36.0" prefWidth="78.0" style="-fx-background-color: #61c4e4;" text="Modificar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
         </children>
      </HBox>
      <ImageView fx:id="imgAtras" fitHeight="45.0" fitWidth="45.0" layoutY="30.0" onMouseClicked="#clickAtras">
         <image>
            <Image url="@../../../../imagen/atras.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
