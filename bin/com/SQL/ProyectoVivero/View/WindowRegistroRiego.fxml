<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="600.0" style="-fx-background-color: #2ca96c;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.SQL.ProyectoVivero.View.RegistroRiegoController">
   <children>
      <TableView fx:id="tableRiego" layoutX="39.0" layoutY="107.0" prefHeight="236.0" prefWidth="523.0" style="-fx-background-color: #61c4e4;">
         <columns>
            <TableColumn fx:id="columnId" prefWidth="88.0" text="ID" />
            <TableColumn fx:id="columnProducto" prefWidth="266.0" text="PRODUCTO" />
            <TableColumn fx:id="columnFecha" prefWidth="168.0" text="FECHA DE RIEGO" />
         </columns>
      </TableView>
      <HBox layoutX="86.0" layoutY="400.0" prefHeight="36.0" prefWidth="429.0" spacing="40.0">
         <children>
            <Button fx:id="btnVer" mnemonicParsing="false" onAction="#clickVer" prefHeight="39.0" prefWidth="77.0" style="-fx-background-color: #61c4e4;" text="Ver">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btnAgregar" mnemonicParsing="false" onAction="#clickCrear" prefHeight="41.0" prefWidth="79.0" style="-fx-background-color: #61c4e4;" text="Agregar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btnBorrar" mnemonicParsing="false" onAction="#clickBorrar" prefHeight="39.0" prefWidth="81.0" style="-fx-background-color: #61c4e4;" text="Borrar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="btnModificar" mnemonicParsing="false" onAction="#clickModificar" prefHeight="42.0" prefWidth="79.0" style="-fx-background-color: #61c4e4;" text="Modificar">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <AnchorPane prefHeight="29.0" prefWidth="600.0" style="-fx-background-color: #61c4e4;">
         <children>
            <Label layoutX="204.0" text="REGISTRO DE RIEGO">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <ImageView fx:id="imgAtras" fitHeight="45.0" fitWidth="45.0" layoutY="30.0" onMouseClicked="#clickAtras">
         <image>
            <Image url="@../../../../imagen/atras.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
